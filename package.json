{
  "name": "lock-in-ai",
  "displayName": "Lock-In AI",
  "description": "Monitor coding activity and analyze code quality",
  "version": "0.1.2",
  "publisher": "Studenthacks",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:lockInAI.dashboard",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lockInAI.showDashboard",
        "title": "Show Lock-In AI Dashboard"
      },
      {
        "command": "lockInAI.startServer",
        "title": "Start Lock-In AI Server"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lock-in-ai",
          "title": "Lock-In AI",
          "icon": "resources/analytics.svg"
        }
      ]
    },
    "views": {
      "lock-in-ai": [
        {
          "type": "webview",
          "id": "lockInAI.dashboard",
          "name": "Lock-In AI Dashboard"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@langchain/openai": "^0.5.7",
    "http": "^0.0.1-security",
    "langchain": "^0.3.24",
    "openai": "^4.96.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.99.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonitakos/STJHacks2025.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adonitakos/STJHacks2025/issues"
  },
  "homepage": "https://github.com/adonitakos/STJHacks2025#readme"
}
